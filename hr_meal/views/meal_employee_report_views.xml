<odoo>
    <record id="view_meal_employee_report_list" model="ir.ui.view">
        <field name="name">meal.employee.report.list</field>
        <field name="model">meal.employee.report</field>
        <field name="arch" type="xml">
            <list>
                <field name="employee_id" widget="many2one_avatar"/>
                <field name="department_id"/>
                <field name="total_meal"/>
                <field name="meal_rate"/>
                <field name="total_bill"/>
            </list>
        </field>
    </record>
</odoo>
